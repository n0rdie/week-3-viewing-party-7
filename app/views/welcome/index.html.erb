<% if @logged_in %>
    <%= link_to "Log Out", '/logout' %>

    <section class='existing-users'>
    <p>Existing Users: </p>
    <% @users.each do |user| %>
        <li><%= user.email %></li>
    <% end %>
</section>
<% else %>
    <%= link_to "Log In", '/login' %>
    <%= button_to "Create New User", register_path, method: :get %>
<% end %>